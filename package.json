{
  "name": "state-engine-cli-kit",
  "version": "1.0.0",
  "description": "StateEngine CLI Kit is a powerful command-line tool for configuring state management in any frontend library or framework configured with TypeScript, reducing repetitive tasks.",
  "main": "index.js",
  "type": "module",
  "bin": {
    "seck": "dist/index.js"
  },
  "scripts": {
    "build": "npx tsc",
    "watch": "npx tsc --watch",
    "clean": "rm -rf dist",
    "relink": "npm unlink -g && npm link",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Samaila Chatto Bashir <developer.chatto@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/parser": "^7.25.6",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "execa": "^9.3.1",
    "fs-extra": "^11.2.0",
    "inquirer": "^10.2.2",
    "recast": "^0.23.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/execa": "^2.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.5.4",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.0",
    "globals": "^15.9.0",
    "husky": "^8.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samaila-bashir/state-engine-cli-kit.git"
  }
}
